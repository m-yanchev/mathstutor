{
  "name": "mathstutor",
  "version": "1.0.0",
  "description": "mathematics tutor",
  "private": true,
  "scripts": {
    "compile about": "tsc --build views/about/tsconfig.json common/tsconfig.json",
    "build about server": "webpack --config views/about/server.webpack.config.js && cd views/about/serverDist/ && zip about.zip about.js",
    "build about client": "webpack --config views/about/client.webpack.config.js",
    "start signup": "webpack serve --config views/signup/dev.webpack.config.js --open",
    "build api": "webpack --config backend/api/webpack.config.js && cd backend/api/dist/ && zip api.zip api.js"
  },
  "repository": {
    "type": "git",
    "url": "mathstutor"
  },
  "author": "Nicholas Yanchev",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-react": "^7.14.5",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "mini-css-extract-plugin": "^2.1.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "apollo-server-lambda": "^3.1.1",
    "babel-preset-react-app": "^3.1.2",
    "graphql": "^15.5.1",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primereact": "^6.5.0-rc.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
